    },
    "scripts": {
        "dev": "cross-env NODE_ENV=local  gulp dev",
        "preview": "cross-env NODE_ENV=preview  gulp dev",
        "init": "npm run build && npm run dev",
        "sprite": "gulp auto-sprite && sleep 1 && gulp auto-sprite",
        "go": "gulp clean  && gulp build  && gulp rev && gulp rev-fix && gulp rev-views",
        "clean": "gulp clean",
        "build": "gulp clean && sleep 1  && gulp build",
        "nocombo-publish": "npm run build && sleep 1 && gulp rev && gulp rev-fix && gulp rev-views",
        "publish-skip": "gulp clean && sleep 1  && gulp build  && sleep 1 && gulp rev --skipV && gulp rev-fix && gulp rev-views && gulp preview-combo  && gulp copy-config",
        "static": "gulp static-html",
        "online-view": "npm run nocombo-publish && gulp static-html --envConf  build && gulp ftp-static && gulp ftp-views",
        "online-preview": "npm run nocombo-publish && gulp static-html --envConf build --previews  && gulp ftp-static --previews && gulp ftp-views --previews",
        "publish-adv": "gulp clean && sleep 1  && gulp build  && sleep 1  && gulp rev && sleep 1 && gulp tmp-store && gulp copy-hash-map && gulp rev-fix && gulp rev-build-all && sleep 1 && gulp deps-update-all && gulp rev-fix-deps && gulp rev-views-deps && gulp rev-build-all && gulp preview-combo --useLogic && gulp clean-ana && gulp copy-config"
    },
